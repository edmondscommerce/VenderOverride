{
    "name": "edmondscommerce/vendor-override",
    "description": "Applies hot fixes to vendor composer packages for when forking is not possible like in Magento 2",
    "keywords": [
        "Magento",
        "Magento 2",
        "Bugs",
        "Issues",
        "Hot Fixes",
        "Patch",
        "Fixes"
    ],
    "homepage": "https://www.edmondscommerce.co.uk",
    "license": "MIT",
    "authors": [
        {
            "name": "edmondscommerce",
            "email": "info@edmondscommerce.co.uk"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "EdmondsCommerce\\VendorOverride\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "league/flysystem": "^1.0"
    }
}
